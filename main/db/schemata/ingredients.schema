create_table "ingredients", id: :bigint, unsigned: true, force: :cascade, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin ROW_FORMAT=DYNAMIC" do |t|
  t.bigint   "recipe_id", null: false
  t.string   "quantity", null: false
  t.string   "name", null: false
  t.integer  "position", null: false
  t.timestamps
end
